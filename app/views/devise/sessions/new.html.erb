
<h2>IPC Gerencia</h2>
<h4>Gesti√≥n de Contratos</h4>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name), html: { :class => 'form-signin', :role => 'form' }) do |f| %>
<div class="form-group"> 
  <%= f.text_field :email, :autofocus => true, :required => '', :placeholder => t('users.email'), :class => 'form-control' %>
</div>
<div class="form-group">  
  <%= f.password_field :password, :placeholder => t('users.password'), autocomplete: "off", :class => 'form-control' %>
</div>  
  <% if devise_mapping.rememberable? -%>
     <label style="display:none;" class="checkbox"><%= f.check_box :remember_me %> <%= f.label t('users.remember_me') %></label>
  <% end -%>
  <%= f.submit t('users.session'), :class => 'btn btn-lg btn-primary btn-block' %><br>
  <%#= render "devise/shared/links" %>
<% end %>
